    package="com.example.android.surgen">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".GenerateSurvey"
            android:label="@string/category_generate" />
        <activity
            android:name=".TakeSurvey"
            android:label="@string/category_take" />
        <activity android:name=".Main2Activity" />
        <activity android:name=".EditSurvey" />
        <activity android:name=".NewSurvey" />
        <activity android:name=".UpdateSurvey" />
        <activity android:name=".DisplayQuestions" />
        <activity android:name=".GiveResponse" />
        <activity android:name=".Response" />
        <activity android:name=".DisplayQuestionsinResponse" />
        <activity android:name=".ViewResponses"></activity>
    </application>

</manifest>